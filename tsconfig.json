{
  /* Visit https://aka.ms/tsconfig to read more about this file */
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "baseUrl": "./",
    "pretty": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "@shared/*": [
        "./src/Shared/*"
      ],
      "@models/*": [
        "./src/Shared/Models/*"
      ],
      "@routers/*": [
        "./src/Server/ApiRouters/*"
      ]
    }
  }
}